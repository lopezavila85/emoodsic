<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.unir.emoodsic.dbaccess.mappers.emoodsic.MoodCategoryMapper">

    <cache />

    <select id="getList"
        resultType="net.unir.emoodsic.common.entities.MoodCategory">
        SELECT idMoodCategory, name, minValence, maxValence, minArousal,
            maxArousal
        FROM emoodsic.moodCategory
    </select>

</mapper>